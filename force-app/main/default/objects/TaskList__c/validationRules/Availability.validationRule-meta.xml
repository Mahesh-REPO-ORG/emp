<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Availability</fullName>
    <active>true</active>
    <errorConditionFormula>AND(Availability__c &gt;= 1 , Availability__c &lt;= 100)</errorConditionFormula>
    <errorDisplayField>Availability__c</errorDisplayField>
    <errorMessage>Availability percent cannot exceed 100%</errorMessage>
</ValidationRule>
